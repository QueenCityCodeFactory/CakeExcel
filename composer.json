{
    "name": "dakota/cake-excel",
    "type": "cakephp-plugin",
    "description": "CakePHP excel generator",
    "keywords": [
        "cakephp",
        "dakota",
        "excel",
        "phpexcel"
    ],
    "homepage": "https://github.com/dakota/CakeExcel",
    "license": "MIT",
    "authors": [
        {
            "name": "Walther Lalk",
            "email": "emailme@waltherlalk.com"
        }
    ],
    "modifier": [
        {
            "name": "Daren Sipes",
            "email": "daren@sipes.me"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "cakephp/cakephp": "~3.5",
        "phpoffice/phpspreadsheet": "~1.2.0"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^3.0",
        "phpunit/phpunit": "^5.7 | ^6.0"
    },
    "autoload": {
        "psr-4": {
            "CakeExcel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeExcel\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "support": {
        "issues": "https://github.com/dakota/CakeExcel/issues",
        "source": "https://github.com/dakota/CakeExcel"
    }
}
